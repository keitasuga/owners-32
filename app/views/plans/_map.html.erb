
<div id="map" style="width:100%;height:401px;"></div>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDBpNfjMqeZJ43fyTzluUuFmkYWnAWOK3Q&callback=initMap"
async defer></script>
<script>
  var map;
  var marker;
  var center = {
    lat: 36.677763, //緯度
    lng: 138.292446 //経度
  };
  function initMap() { //id="map"の位置に地図を生成
    map = new google.maps.Map(document.getElementById('map'), {
      center: center,
      zoom: 8
    });
    marker = new google.maps.Marker({ //マーカーの表示
      position: center,
      map: map
    });
  }
</script>
